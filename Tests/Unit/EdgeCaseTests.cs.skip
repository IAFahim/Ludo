        // ========== Movable Tokens Mask Edge Cases ==========

        [Test]
        public void GetMovableTokens_AllTokensAtBase_OnlyWithSix()
        {
            var board = LudoBoard.Create(2);
            
            var result = board.TryGetMovableTokens(0, 6);
            Assert.That(success, Is.True);
            Assert.That(captured, Is.EqualTo(MovableTokens.T0 | MovableTokens.T1 | MovableTokens.T2 | MovableTokens.T3));
            
            var resultNonSix = board.TryGetMovableTokens(0, 3);
            Assert.That(resultNonSix.IsOk, Is.True);
            Assert.That(resultNonSix.Unwrap(), Is.EqualTo(MovableTokens.None));
        }

        [Test]
        public void GetMovableTokens_AllTokensHome_ReturnsNone()
        {
            var board = LudoBoard.Create(2);
            board.TokenPositions[0] = 57;
            board.TokenPositions[1] = 57;
            board.TokenPositions[2] = 57;
            board.TokenPositions[3] = 57;
            
            var result = board.TryGetMovableTokens(0, 6);
            Assert.That(success, Is.True);
            Assert.That(captured, Is.EqualTo(MovableTokens.None));
        }

        [Test]
        public void GetMovableTokens_TokenWouldOvershoot_NotInMask()
        {
            var board = LudoBoard.Create(2);
            board.TokenPositions[0] = 56; // Would overshoot with dice > 1
            
            var result = board.TryGetMovableTokens(0, 2);
            Assert.That(success, Is.True);
            Assert.That(captured.HasFlag(MovableTokens.T0), Is.False);
            
            var resultExact = board.TryGetMovableTokens(0, 1);
            Assert.That(resultExact.IsOk, Is.True);
            Assert.That(resultExact.Unwrap().HasFlag(MovableTokens.T0), Is.True);
        }

        // ========== IsTokenMovable Edge Cases ==========

        [Test]
        public void State_IsTokenMovable_AllIndices()
        {
            var state = LudoState.Create(2);
            state.RecordDiceRoll(5, MovableTokens.T1 | MovableTokens.T3);
            
            // Test that MovableTokensMask correctly identifies movable tokens
            Assert.That((state.MovableTokensMask & MovableTokens.T0), Is.EqualTo(MovableTokens.None));
            Assert.That((state.MovableTokensMask & MovableTokens.T1), Is.Not.EqualTo(MovableTokens.None));
            Assert.That((state.MovableTokensMask & MovableTokens.T2), Is.EqualTo(MovableTokens.None));
            Assert.That((state.MovableTokensMask & MovableTokens.T3), Is.Not.EqualTo(MovableTokens.None));
        }

